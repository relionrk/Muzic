<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rkrelion.muzic">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Firebase">
        <activity android:name="com.rkrelion.muzic.myFavourites"></activity>
        <activity android:name="com.rkrelion.muzic.UserFav" />
        <activity android:name="com.rkrelion.muzic.PlayList" />
        <activity android:name="com.rkrelion.muzic.PlaySong" />
        <activity android:name="com.rkrelion.muzic.local" />
        <activity android:name="com.rkrelion.muzic.user_activiy" />
        <activity android:name="com.rkrelion.muzic.userlist" />
        <activity android:name="com.rkrelion.muzic.upload" />
        <activity android:name="com.rkrelion.muzic.myProfile" >android:theme="@style/Theme.AppCompat.Light"</activity>
        <activity android:name="com.rkrelion.muzic.signIn" />
        <activity android:name="com.rkrelion.muzic.signUp" />
        <activity android:name="com.rkrelion.muzic.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>